@using CustomWebApp.Models;
@model CustomWebApp.Models.Results
@{
    Layout = "_Layout";
    ViewData["Title"] = "Search";
}
<div>
    <h2>@Model.Message</h2>
    @if(Model.Items == null || ((List<string>)Model.Items).Count() == 0) {
        <p>Zero results.</p>
    } else {
        <table class="res">
            <tr><th>Item</th></tr>
            @foreach(string item in Model.Items) {
                <tr><td>@Html.Raw(@item)</td></tr>
            }
        </table>
    }
</div>